{
  "name": "monolith",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node ace serve --watch",
    "build": "node ace build --production",
    "start": "node server.js",
    "test": "node ace test",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write ."
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.9.5",
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@japa/preset-adonis": "^1.2.0",
    "@japa/runner": "^2.5.1",
    "@symfony/webpack-encore": "^4.1.1",
    "@types/lodash": "^4.14.194",
    "@types/proxy-addr": "^2.0.0",
    "@types/source-map-support": "^0.5.6",
    "adonis-preset-ts": "^2.1.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-adonis": "^2.1.1",
    "eslint-plugin-prettier": "^4.2.1",
    "pino-pretty": "^10.0.0",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.0",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.3.2",
    "typescript": "~4.6",
    "webpack": "^5.82.1",
    "webpack-cli": "^4.10.0",
    "youch": "^3.2.3",
    "youch-terminal": "^2.2.0"
  },
  "dependencies": {
    "@adonisjs/auth": "^8.2.3",
    "@adonisjs/core": "^5.9.0",
    "@adonisjs/limiter": "^1.0.2",
    "@adonisjs/lucid": "^18.3.0",
    "@adonisjs/repl": "^3.1.11",
    "@adonisjs/session": "^6.4.0",
    "@adonisjs/shield": "^7.1.0",
    "@adonisjs/view": "^6.2.0",
    "@google-cloud/vision": "^3.1.3",
    "@iconify-json/ph": "^1.1.5",
    "alpinejs": "^3.12.1",
    "daisyui": "^2.51.6",
    "edge-iconify": "^1.0.2-0",
    "html-to-image": "^1.11.11",
    "lazysizes": "^5.3.2",
    "lodash": "^4.17.21",
    "luxon": "^3.3.0",
    "nanoid": "^3.3.6",
    "node-vibrant": "^3.2.1-alpha.1",
    "openai": "^3.2.1",
    "pg": "^8.10.0",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "replicate": "^0.12.0",
    "sharp": "^0.32.1",
    "source-map-support": "^0.5.21",
    "sqlite3": "^5.1.6"
  },
  "browserslist": [
    "last 2 versions"
  ]
}
